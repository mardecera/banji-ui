{
	"name": "banji-ui",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"files": [
		"dist"
	],
	"main": "./dist/banji-ui.umd.cjs",
	"module": "./dist/banji-ui.js",
	"sideEffects": false,
	"exports": {
		".": {
			"import": "./dist/banji-ui.js",
			"require": "./dist/banji-ui.umd.cjs"
		},
		"./style.css": "./dist/banji-ui.css"
	},
	"scripts": {
		"dev": "vite",
		"build": "tsc -b && vite build",
		"lint": "eslint .",
		"preview": "vite preview",
		"biome": "biome check ./lib",
		"biome:fix": "biome check --write ./lib",
		"test": "vitest run"
	},
	"dependencies": {
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"vite-plugin-dts": "^4.5.4",
		"vite-plugin-lib-inject-css": "^2.2.2",
		"vite-tsconfig-paths": "^5.1.4"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.0",
		"@testing-library/jest-dom": "^6.8.0",
		"@testing-library/react": "^16.3.0",
		"@testing-library/user-event": "^14.6.1",
		"@types/node": "^24.3.0",
		"@types/react": "^19.1.10",
		"@types/react-dom": "^19.1.7",
		"@vitejs/plugin-react-swc": "^4.0.0",
		"globals": "^16.3.0",
		"jsdom": "^26.1.0",
		"typescript": "~5.8.3",
		"vite": "^7.1.2",
		"vitest": "^3.2.4"
	}
}